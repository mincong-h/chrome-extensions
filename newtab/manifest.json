{
  "name": "Mincong new tab page",
  "description": "Override the new tab page with Mincong style",
  "version": "0.2",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "blank.html"
  },
  "manifest_version": 2,
  "permissions": [
    "bookmarks",
    "https://www.nasa.gov/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://www.nasa.gov/; object-src 'self'"
}
